@model Mvckutuphane.Models.Entity.TBLPERSONEL
@{
    ViewBag.Title = "PersonelGetir";
    Layout = "~/Views/Shared/Layout.cshtml";
}

@using (Html.BeginForm("PersonelGuncelle", "personel", FormMethod.Post))
{
    <div style="margin-bottom:15px"></div>
    <div class="form-group">
        @Html.LabelFor(x =>x.ID)
        @Html.TextBoxFor(x => x.ID, new { @class = "form-control" })
        <br />

        @Html.LabelFor(x => x.PERSONEL)
        @Html.TextBoxFor(x => x.PERSONEL, new { @class = "form-control" })
    </div>
    <div>
        <button class="btn btn-warning">Güncelle</button>
    </div>

}   